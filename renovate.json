{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":semanticCommitsDisabled"
  ],
  "labels": [
    "Renovate"
  ],
  "rebaseWhen": "conflicted",
  "packageRules": [
    {
      "matchPackageNames": ["net.minestom:minestom-snapshots"],
      "enabled": false
    },
    {
      "matchUpdateTypes": [
        "patch"
      ],
      "automerge": true
    }
  ]
}
